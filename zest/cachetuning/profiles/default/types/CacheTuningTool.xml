<?xml version="1.0"?>
<object name="CacheTuningTool"
        meta_type="Factory-based Type Information with dynamic views"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n"
   i18n:domain="plone">
 <property i18n:translate="cachetuningtool-contenttype-title"
           name="title">CacheTuningTool</property>
 <property name="description"></property>
 <property name="content_icon"></property>
 <property name="content_meta_type">CacheTuningTool</property>
 <property name="product">zest.cachetuning</property>
 <property name="factory">addCacheTuningTool</property>
 <property name="immediate_view">atct_edit</property>
 <property name="global_allow">True</property>
 <property name="allow_discussion">False</property>
 <property name="default_view">atct_edit</property>
 <property name="view_methods">
